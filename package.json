{
	"$schema": "http://json.schemastore.org/package",
	"name": "@chatsift/automoderator",
	"packageManager": "yarn@4.3.1",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"workspaces": [
		"packages/*",
		"services/*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/didinele/AutoModerator.git"
	},
	"author": "didinele",
	"bugs": {
		"url": "https://github.com/didinele/AutoModerator/issues"
	},
	"homepage": "https://github.com/didinele/AutoModerator#readme",
	"scripts": {
		"lint": "turbo run lint && prettier --check .",
		"build": "turbo run build",
		"format": "prettier --write .",
		"postinstall": "is-ci || husky install || true",
		"update": "yarn upgrade-interactive",
		"prisma": "DATABASE_URL=postgresql://chatsift:admin@localhost:5432/chatsift prisma"
	},
	"dependencies": {
		"prisma": "^5.16.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-angular": "^19.3.0",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.57.0",
		"eslint-config-neon": "^0.1.62",
		"eslint-formatter-pretty": "^6.0.1",
		"husky": "^9.0.11",
		"is-ci": "^3.0.1",
		"lodash.merge": "^4.6.2",
		"prettier": "^3.3.2",
		"prisma-kysely": "^1.8.0",
		"rimraf": "^5.0.9",
		"turbo": "^2.0.6",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.16.0"
	},
	"resolutions": {
		"discord-api-types": "0.37.84"
	}
}
